# Docker Compose Configuration
COMPOSE_PROJECT_NAME=wishlist

# Timezone for containers
TZ=America/New_York

# Frontend Configuration
# In Docker, nginx proxies requests so we use relative URLs
VITE_POCKETBASE_URL=/

# SSL Configuration (required for HTTPS)
# Your No-IP DDNS domain (e.g., yourname.ddns.net)
DOMAIN_NAME=your-domain.ddns.net

# Email for Let's Encrypt certificate notifications
LETSENCRYPT_EMAIL=your-email@example.com
